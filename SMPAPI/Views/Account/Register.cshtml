    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="keywords" content="Dedicated to Legend Shushant Singh Rajput,Website" />

    <script>
        addEventListener("load", function () {
            setTimeout(hideURLbar, 0);
        }, false);

