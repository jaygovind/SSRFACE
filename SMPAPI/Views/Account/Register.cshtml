    <meta name="robots" content="noindex">

    <link rel="stylesheet" href="~/css/demobar_w3_4thDec2019.css">
    <!-- Demo bar start -->
    <div id="w3lDemoBar" class="w3l-demo-bar">

        <p style="color:white">Welcome To SSR</p>
        <div class="w3l-template-options">
            <p style="color:white">This website dedicated to Legend Shushant Singh Rajput</p>
        </div>
    </div>

    <!-- main -->
    <div class="w3layouts-main">
        <div class="bg-layer">
            <h1 style="color:yellowgreen">Welcome To SSRFACE</h1>
            <!---728x90--->

            <div class="header-main">
                <div class="main-icon">
                    <span class="fa fa-eercast"></span>
                </div>
                <div class="header-left-bottom">

                    <div class="links tab-group">
                        <p class="tab "><a href="#sigin">Sign IN</a></p>
                        <p class="right tab active"><a href="#signup">New User? Register</a></p>
                        <div class="clear"></div>
                    </div>
                    <div class="tab-content">
                        <div id="signup">
                            <form action="/Account/Register" method="post">
                                <div class="icon1">
                                    <span class="fa fa-user"></span>
                                    <input type="email" name="emailRegsiter" id="emailRegsiter" placeholder="Email Address" required="" />
                                </div>
                                <div class="icon1">
                                    <span class="fa fa-user"></span>
                                    <input type="text" name="FirstName" id="FirstName" placeholder="First Name" required="" />
                                </div>
                                <div class="icon1">
                                    <span class="fa fa-lock"></span>
                                    <input type="password" name="regpassword" id="regpassword" placeholder="Password" required="" />
                                </div>
                                <div class="icon1">
                                    <span class="fa fa-lock"></span>
                                    <input type="password" name="regconpasswrd" id="regconpasswrd" placeholder="Confirm Password" required="" />
                                </div>
                                <div class="icon1">
                                    Male: <input checked="checked"
                                                 id="Gender"
                                                 name="Gender"
                                                 type="radio"
                                                 value="Male" />

                                    Female: <input id="Gender"
                                                   name="Gender"
                                                   type="radio"
                                                   value="Female" />
                                    Other: <input id="Gender"
                                                  name="Gender"
                                                  type="radio"
                                                  value="Other" />
                                </div>

                                <div class="bottom">
                                    <button type="submit" class="btn">Register</button>
                                </div>

                            </form>
                        </div>
                        <div id="sigin">
                            <form action="/Account/Login" method="post">
                                <div class="icon1">
                                    <span class="fa fa-user"></span>
                                    <input type="email" name="emailLogin" id="emailLogin" placeholder="Email Address" required="" />
                                </div>
                                <div class="icon1">
                                    <span class="fa fa-lock"></span>
                                    <input type="password" name="LoginPaaswrd" id="LoginPaaswrd" placeholder="Password" required="" />
                                </div>
                                @*<div class="login-check">
                                <label class="checkbox"><input type="checkbox" name="checkbox" checked=""><i> </i> Keep me logged in</label>
                            </div>*@
                                <div class="bottom">
                                    <button type="submit" class="btn">Log In</button>
                                </div>
                                <div class="links">
                                    <p><a href="#">Forgot Password?</a></p>
                                    <p class="right"><a href="#">New User? Register</a></p>
                                    <div class="clear"></div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="social">
                    <ul>
                        <li>or login using : </li>
                        <li><a href="#" class="facebook"><span class="fa fa-facebook"></span></a></li>
                        <li><a href="#" class="twitter"><span class="fa fa-twitter"></span></a></li>
                        <li><a href="#" class="google"><span class="fa fa-google-plus"></span></a></li>
                    </ul>
                </div>
            </div>
            <!---728x90--->
            <!-- copyright -->
            <div class="copyright">
                <p>© 2019 Dedicated To Shushant Singh Rajput . All rights reserved | Design by <a href="" target="_blank">Team</a></p>
            </div>
            <!-- //copyright -->
        </div>
    </div>
    <!-- //main -->

</body>
</html>
<script type="text/javascript" src="//cdn.jsdelivr.net/jquery/1/jquery.min.js"></script>
<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-growl/1.0.0/jquery.bootstrap-growl.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
<script>
    $('.tab a').on('click', function (e) {

        e.preventDefault();

        $(this).parent().addClass('active');
        $(this).parent().siblings().removeClass('active');

        target = $(this).attr('href');

        $('.tab-content > div').not(target).hide();

        $(target).fadeIn(600);

    });

    if ('@ViewBag.data' != "") {
        debugger;
        $.bootstrapGrowl('@ViewBag.data', {
            type: 'success',
            delay: 3000,
        });

            Swal.fire({
  title: "Info",
  html: "@ViewBag.data",
  confirmButtonText: "Ok",
});
    }


</script>

